{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "dev Express",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "dev",
        "-r",
        "dotenv/config",
        "--inspect",
        "--env",
        "NODE_ENV=development"
      ],
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
